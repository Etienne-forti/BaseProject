<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircularProgressView">
        <attr name="cpv_progressColor" format="color"/>
        <attr name="cpv_border_width" format="dimension"/>
        <attr name="cpv_height_accent" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ExRecyclerView">
        <attr name="erv_backgroundDrawable" format="reference"/>
        <attr name="erv_progressColor" format="color"/>
        <attr name="erv_progressBorderWidth" format="dimension"/>
        <attr name="erv_progressSize" format="dimension"/>
        <attr name="erv_textSize" format="dimension"/>
        <attr name="erv_textColor" format="color"/>
        <attr name="erv_textDefault" format="string"/>
        <attr name="erv_textPadding" format="dimension"/>
        <attr name="erv_background_duration" format="integer"/>
    </declare-styleable>

    <declare-styleable name="ProgressButtonView">
        <attr name="pbv_progressColor" format="color"/>
        <attr name="pbv_progressSize" format="dimension"/>
        <attr name="pbv_progressBorderWidth" format="dimension"/>
        <attr name="pbv_progressPadding" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="AspectHelper">
        <attr name="aspect_width" format="integer"/>
        <attr name="aspect_height" format="integer"/>
        <attr name="aspect_by_height" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="SquareImageView">
        <attr name="siv_by_height" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="VectorCompatTextView">
        <attr name="drawableStartCompat" format="reference"/>
        <attr name="drawableTopCompat" format="reference"/>
        <attr name="drawableEndCompat" format="reference"/>
        <attr name="drawableBottomCompat" format="reference"/>
        <attr name="drawableWidth" format="dimension"/>
        <attr name="drawableHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="PlacesAutoCompleteView">
        <attr name="gac_debounce" format="integer"/>
        <attr name="gac_min_start_search" format="integer"/>
        <attr name="gac_max_result" format="integer"/>
        <attr name="gac_init_adapter" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CircularRevealTransition">
        <attr name="startRadius" format="dimension"/>
        <attr name="endRadius" format="dimension"/>
        <attr name="centerOn" format="reference"/>
    </declare-styleable>

    <declare-styleable name="FontTextView">
        <attr name="font" format="string"/>
        <attr name="textStyle" format="integer"/>
    </declare-styleable>

</resources>