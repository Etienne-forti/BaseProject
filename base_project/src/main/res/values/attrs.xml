<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircularProgressView">
        <attr name="cpv_progressColor" format="color"/>
        <attr name="cpv_border_width" format="dimension"/>
        <attr name="cpv_height_accent" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ExRecyclerView">
        <attr name="erv_progressColor" format="color"/>
        <attr name="erv_progressBorderWidth" format="dimension"/>
        <attr name="erv_progressSize" format="dimension"/>
        <attr name="erv_textSize" format="dimension"/>
        <attr name="erv_textColor" format="color"/>
        <attr name="erv_textDefault" format="string"/>
        <attr name="erv_textPadding" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SquareImageView">
        <attr name="siv_by_height" format="boolean"/>
    </declare-styleable>


</resources>